STRIPE_WEBHOOK_SECRET=$(grep STRIPE_WEBHOOK_SECRET .env.production | cut -d= -f2)
STRIPE_WORKER_MONTHLY_PRICE_ID=$(grep STRIPE_WORKER_MONTHLY_PRICE_ID .env.production | cut -d= -f2)
STRIPE_WORKER_YEARLY_PRICE_ID=$(grep STRIPE_WORKER_YEARLY_PRICE_ID .env.production | cut -d= -f2)
STRIPE_ENTERPRISE_MONTHLY_PRICE_ID=$(grep STRIPE_ENTERPRISE_MONTHLY_PRICE_ID .env.production | cut -d= -f2)
STRIPE_ENTERPRISE_YEARLY_PRICE_ID=$(grep STRIPE_ENTERPRISE_YEARLY_PRICE_ID .env.production | cut -d= -f2)
