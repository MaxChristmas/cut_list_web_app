# Stripe test keys for staging
# Reads from .env.staging on your local machine at deploy time
STRIPE_SECRET_KEY=$(grep STRIPE_SECRET_KEY .env.staging | cut -d= -f2)
STRIPE_WEBHOOK_SECRET=$(grep STRIPE_WEBHOOK_SECRET .env.staging | cut -d= -f2)
STRIPE_WORKER_MONTHLY_PRICE_ID=$(grep STRIPE_WORKER_MONTHLY_PRICE_ID .env.staging | cut -d= -f2)
STRIPE_WORKER_YEARLY_PRICE_ID=$(grep STRIPE_WORKER_YEARLY_PRICE_ID .env.staging | cut -d= -f2)
STRIPE_ENTERPRISE_MONTHLY_PRICE_ID=$(grep STRIPE_ENTERPRISE_MONTHLY_PRICE_ID .env.staging | cut -d= -f2)
STRIPE_ENTERPRISE_YEARLY_PRICE_ID=$(grep STRIPE_ENTERPRISE_YEARLY_PRICE_ID .env.staging | cut -d= -f2)
